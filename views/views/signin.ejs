    <link rel="stylesheet" href="/styles.css">
    <link rel="stylesheet" href="/sign.css">
</head>
<body >
 
    
    <!-- CenterSport Logo -->
    <nav class="navbar navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/index">
            <img src="/images/trophy.png" alt="" width="30" height="25" class="d-inline-block align-top">
            AllSports
          </a>
        </div>
      </nav>

    <!-- End of Navbar -->

    <!-- Text Card -->
    
    <h6 style="margin: 30px 0 0 0; text-align: center;"><%= error %> </h6>      
    <div class="login-box" style="margin: 50px auto;">
        <h2>Sign In</h2>
        <form method="POST" action="/complete-signin" autocomplete="off">
            <div class="user-box">
            <input type="text" name="username" required=""><label>Username</label>
            </div>
            <div class="user-box">
            <input type="password" name="p1" required="" id="p1"><label>Password</label>
            </div>
            <input type="submit" class="submit">
        </form>
    </div>     

    <!-- End of text card -->
</body>
